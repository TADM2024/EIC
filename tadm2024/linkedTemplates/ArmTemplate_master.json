{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tadm2024"
		},
		"LS_SQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLDB'"
		},
		"LS_StorageAccount_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_StorageAccount'"
		},
		"LS_SQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tadm2024.database.windows.net"
		},
		"LS_SQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ADF_DEMO"
		},
		"LS_SQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admintadm"
		},
		"StorageEventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d38e295c-935f-422a-b557-6b00aee34e0e/resourceGroups/TadmTraining2024/providers/Microsoft.Storage/storageAccounts/blob2024tadm"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tadm2024_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQLDB_password": {
						"value": "[parameters('LS_SQLDB_password')]"
					},
					"LS_StorageAccount_connectionString": {
						"value": "[parameters('LS_StorageAccount_connectionString')]"
					},
					"LS_SQLDB_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLDB_properties_typeProperties_server')]"
					},
					"LS_SQLDB_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLDB_properties_typeProperties_database')]"
					},
					"LS_SQLDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLDB_properties_typeProperties_userName')]"
					},
					"StorageEventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('StorageEventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}